        <Param val="grids = [grid1_2, grid2_2, grid3_2, grid4_2, grid5_2, grid6_2, grid7_2, grid8_2, grid9_2];&amp;#10;resp_2.pos = [(- 0.5), (- 0.25)];&amp;#10;grid_dropped = &quot;&quot;;&amp;#10;showSubmit = false;&amp;#10;correct = 0;&amp;#10;time_taken = 0;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# create a list of grids to check where the square has been dropped&amp;#10;grids = [grid1_2, grid2_2, grid3_2, grid4_2, grid5_2, grid6_2, grid7_2, grid8_2, grid9_2]&amp;#10;&amp;#10;# reset the response square at the beginning of each trial&amp;#10;resp_2.pos = [-0.5, -0.25]&amp;#10;&amp;#10;# variable to save which grid the square was dropped in&amp;#10;grid_dropped = &quot;&quot;&amp;#10;&amp;#10;# only show the submit button when a square has been dropped&amp;#10;showSubmit = False&amp;#10;&amp;#10;# variable to store if the trial was correct&amp;#10;correct = 0&amp;#10;&amp;#10;# variable to store time taken to complete trial&amp;#10;time_taken = 0" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="# create dragging effect&amp;#10;if mouse_2.isPressedIn(resp_2):&amp;#10;    resp_2.pos = mouse_2.getPos()&amp;#10;&amp;#10;# check each grid&amp;#10;for grid in grids:&amp;#10;    # if mouse is hovering over each grid&amp;#10;    if grid.contains(mouse_2):&amp;#10;        # if the left click is pressed down&amp;#10;        if mouse_2.getPressed()[0]:&amp;#10;            # drop/snap the response square to that grid&amp;#10;            resp_2.setPos(grid.pos)&amp;#10;            # fetch which grid the square was dropped in&amp;#10;            grid_dropped = grid.name&amp;#10;            # show the submit button&amp;#10;            showSubmit = True&amp;#10;&amp;#10;# end the routine when the submit button is clicked&amp;#10;# and record how long it took to complete trial&amp;#10;if mouse_2.isPressedIn(submit_button_2) and t &gt; 1:&amp;#10;    time_taken = t # fetch current time of trial&amp;#10;    continueRoutine = False" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="if (mouse_2.isPressedIn(resp_2)) {&amp;#10;    resp_2.pos = mouse_2.getPos();&amp;#10;}&amp;#10;for (var grid, _pj_c = 0, _pj_a = grids, _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    grid = _pj_a[_pj_c];&amp;#10;    if (grid.contains(mouse_2)) {&amp;#10;        if (mouse_2.getPressed()[0]) {&amp;#10;            resp_2.setPos(grid.pos);&amp;#10;            grid_dropped = grid.name;&amp;#10;            showSubmit = true;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if ((mouse_2.isPressedIn(submit_button_2) &amp;&amp; (t &gt; 1))) {&amp;#10;    time_taken = t;&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
